spring.application.name=heroes_of_code_backend

spring.datasource.driver-class-name=org.postgresql.Driver




spring.jpa.properties.hibernate.format_sql=true
spring.datasource.url = jdbc:postgresql://localhost:5432/ nazwa bazy danych
spring.datasource.username= nazawa użytkownika w bazie danych ( postgres)
spring.datasource.password= hasło do bazy danych
spring.jpa.show-sql=true